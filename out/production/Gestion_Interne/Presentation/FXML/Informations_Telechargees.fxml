<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>
<Pane xmlns:fx="http://javafx.com/fxml/1" fx:id="backgroundPane" prefHeight="185.0" prefWidth="894.0"
      style="-fx-background-color: transparent;" xmlns="http://javafx.com/javafx/10.0.1"
      fx:controller="Presentation.Controller.Informations_Telechargees_Controller">
    <children>
        <Pane fx:id="pane" layoutX="14.0" layoutY="56.0" prefHeight="127.0" prefWidth="838.0"
              style="-fx-border-color: #ff8635; -fx-border-radius: 100; -fx-border-width: 4px 4px 4px 4px; -fx-background-color: f8f6ca; -fx-background-radius: 100;">
            <children>
                <Label layoutX="65.0" layoutY="32.0" prefHeight="35.0" prefWidth="332.0"
                       text="Informations Téléchargées " textFill="#ff8635">
                    <font>
                        <Font name="System Bold" size="24.0"/>
                    </font>
                </Label>
                <ImageView fitHeight="30.0" fitWidth="41.0" layoutX="35.0" layoutY="37.0" pickOnBounds="true"
                           preserveRatio="true">
                    <image>
                        <Image url="@../Assets/Forward.png"/>
                    </image>
                </ImageView>
            </children>
        </Pane>
        <JFXButton fx:id="close" contentDisplay="CENTER" layoutX="827.0" layoutY="31.0" onAction="#fermerFenetre"
                   prefHeight="51.0" prefWidth="51.0"
                   style="-fx-border-color: #ff8635; -fx-border-radius: 100; -fx-background-radius: 100;"
                   styleClass="hover-CloseButton" stylesheets="@../CSSstyling/Style.css" textFill="TRANSPARENT">
            <graphic>
                <ImageView fitHeight="31.0" fitWidth="33.0">
                    <image>
                        <Image url="@../Assets/Close2.png"/>
                    </image>
                </ImageView>
            </graphic>
        </JFXButton>
    </children>
</Pane>
