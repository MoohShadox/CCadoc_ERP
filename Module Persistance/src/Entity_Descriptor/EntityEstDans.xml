<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="Database_Entity.EntityEstDans">
        <table name="EST_DANS" schema="CADOC_ADMIN" catalog=""/>
        <id-class class="Database_Entity.EntityEstDansPK"/>
        <attributes>
            <id name="isbn">
                <column name="ISBN" length="15"/>
            </id>
            <id name="nomS">
                <column name="NOM_S" length="25"/>
            </id>
            <basic name="quantite">
                <column name="QUANTITE" nullable="false" precision="10"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
